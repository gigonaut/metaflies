<div id="<%= data.messageId %>" class="message">
	<p class="poster"><%= data.nickname %> posted:</p>
	<p><%= data.message %></p>
	<div class="replies">
		<div class="post_reply">
			<form action="#" class="reply_form">
				<textarea class="reply" name="reply"></textarea>
				<p><input type="submit" value="+reply"></p>
			</form>
		</div>
	</div>
</div>